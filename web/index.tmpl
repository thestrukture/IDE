        width: 100%%;
        display: block;
    }

    input,
    textarea,
    select {
        border: 1px solid #7d0808 !important;
        background: #656565 !important;
        font-weight: bold !important;
        color: #fff !important;
    }

    .jqx-kanban-column-header,
    .jqx-kanban-column-header-collapsed {
        background: #375a7f;
    }

    .jqx-kanban-item-keyword {
        padding: 5px;
        background: #333;
    }

